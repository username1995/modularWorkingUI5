<mvc:View
	controllerName="ui.demo.multiComponent.app.sub7component.controller.App"
	displayBlock="true"
	height="100%"
	xmlns="sap.m"
	xmlns:f="sap.f"
	xmlns:mvc="sap.ui.core.mvc">
	<App id="app"
	busy="{appView>/busy}"
	busyIndicatorDelay="{appView>/delay}">
	<f:FlexibleColumnLayout
		id="flexibleColumnLayout"
		backgroundDesign="Solid"
		layout="{/layout}">
	</f:FlexibleColumnLayout>
	</App>
</mvc:View>